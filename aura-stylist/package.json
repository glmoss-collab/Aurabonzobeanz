{
  "name": "aura-stylist",
  "private": true,
  "version": "1.0.0",
  "description": "AI-powered virtual wardrobe assistant that analyzes clothing items and generates outfit suggestions",
  "author": "Aura Stylist Team",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:prod": "NODE_ENV=production npm run build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "cap:add": "cap add",
    "cap:sync": "cap sync",
    "cap:copy": "cap copy",
    "cap:update": "cap update",
    "cap:ios": "cap open ios",
    "cap:android": "cap open android",
    "cap:run:ios": "npm run build && cap sync ios && cap open ios",
    "cap:run:android": "npm run build && cap sync android && cap open android",
    "ios:build": "npm run build && cap sync ios",
    "android:build": "npm run build && cap sync android",
    "mobile:init": "npm run build && cap add ios && cap add android && cap sync",
    "mobile:sync": "npm run build && cap sync"
  },
  "dependencies": {
    "@capacitor/android": "^6.0.0",
    "@capacitor/app": "^6.0.0",
    "@capacitor/camera": "^6.0.0",
    "@capacitor/core": "^6.0.0",
    "@capacitor/filesystem": "^6.0.0",
    "@capacitor/ios": "^6.0.0",
    "@capacitor/splash-screen": "^6.0.0",
    "@capacitor/status-bar": "^6.0.0",
    "@google/genai": "^1.35.0",
    "lucide-react": "^0.562.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@capacitor/cli": "^6.0.0",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
